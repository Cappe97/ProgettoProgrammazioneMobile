<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.TortaAnnoFragment">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="torta anno"
        app:layout_constraintBottom_toTopOf="@+id/btn_fromTortaAnno_toTortaMese"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_fromTortaAnno_toTortaMese"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="mese"
        app:layout_constraintBottom_toTopOf="@+id/btn_fromTortaAnno_toTortaSettimana"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/btn_fromTortaAnno_toTortaSettimana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="settimana"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_fromTortaAnno_toTortaMese" />

</androidx.constraintlayout.widget.ConstraintLayout>